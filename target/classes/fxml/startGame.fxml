<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.StartGame"
            prefHeight="720.0" prefWidth="1280.0" stylesheets="@../CSS/style.css">
    <center>
        <VBox alignment="CENTER" spacing="10">
            <Button styleClass="MainMenuButton" text="Easy" onAction="#Easy"/>
            <Button styleClass="MainMenuButton" text="Normal" onAction="#Normal"/>
            <Button styleClass="MainMenuButton" text="Hard" onAction="#Hard"/>
            <Text text=" "/>
            <Button fx:id="backButton" styleClass="MainMenuButton" text="Back" onAction="#backClicked"/>
        </VBox>
    </center>


</BorderPane>
