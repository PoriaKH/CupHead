<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.Results"
            prefHeight="720.0" prefWidth="1280.0" stylesheets= "@../CSS/style.css">
    <center>
        <VBox alignment="CENTER" spacing="5" maxWidth="350">
            <Button fx:id="retryButton" text="Retry" onAction="#RetryPressed"/>
            <Button fx:id="backToMainButton" text="Back to MainMenu" onAction="#BackToMainPressed"/>
            <Text fx:id="message" text=" "/>
        </VBox>
    </center>

</BorderPane>
